{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "LookingOverTheHedge.Api": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "Health",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/Talks",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}